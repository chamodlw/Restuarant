{
  "expo": {
    "name": "Restuarant",
    "slug": "Restuarant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bun.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bun.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE" 
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
